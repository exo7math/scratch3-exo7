\usetikzlibrary{patterns}
\begin{tikzpicture}

\tikzstyle{edge} = [draw, thick]
\tikzstyle{vertex} = [draw] 

\begin{scope}[rotate=0]

\foreach \x in {0,1,...,2}{
  \draw[edge] (\x,0) -- (\x+1,0);
}

  \draw[edge] (1,0) -- (2,1)--(2,0)--(2,-1);

\foreach \x in {0,1,...,3}{
  \fill (\x,0) circle (0.1);
}

  \fill (2,1) circle (0.1);
  \fill (2,-1) circle (0.1);

\end{scope}

\end{tikzpicture}